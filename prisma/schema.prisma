generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Donor {
  id         Int      @id @default(autoincrement())
  name       String
  email      String   @unique
  phone      String
  bloodGroup String
  area       String
  createdAt  DateTime @default(now())
}

